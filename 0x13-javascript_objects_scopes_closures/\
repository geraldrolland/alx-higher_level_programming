#!/usr/bin/node
class Rectangle {
  constructor (w, h) {
    if ((h > 0) && (w > 0)) {
      this.width = w;
      this.height = h;
    }
  }

  print () {
    for (let i = this.height; i >= 1; i--) {
      const x = 'X';
      console.log(x.repeat(this.width));
    }
  }
}

module.exports = Rectangle;
